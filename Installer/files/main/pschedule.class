        class pschedule {
		        double nblock
				double issub
				double bun
				double str
				double bcut
				double sfee
				array  oin
				array  blk        
        }
        
	    program .new
                        if "`0'" != "" {
                                .set `0'
                        }
        end

        program .set
                         args nblock issub bun str bcut sfee
                         .nblock    =  `nblock'
						 .issub     =  `issub'
						 .bun       =  `bun'
						 .str       =  `str'
						 .bcut      =  `bcut'
						 .sfee      =  `sfee'
        end

		 

	 

				
       
