        class range {
                         double min
                         double max
			             double tax
        }
		
	program .new
                        if "`0'" != "" {
                                .set `0'
                        }
        end

        program .set
                     args min max tax 
                         .min     = `min'
                         .max     = `max'
			             .tax     = `tax'

        end
