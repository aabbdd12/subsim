        class itschedule {
		        double nrange
				double exemp
				double str
				double bun
				array  oin
				array  ran        
        }
        
	    program .new
                        if "`0'" != "" {
                                .set `0'
                        }
        end

        program .set
                         args nrange exemp str bun
                         .nrange   =  `nrange'
						 .exemp   =  `exemp'
						 .str      =  `str'
						 .bun      =  `bun'
        end

		 

	 

				
       
